    "import asyncio\n",
    "\n",
    "async def my_coroutine():\n",
    "    print(\"Coroutine started\")\n",
    "    await asyncio.sleep(1)\n",
    "    print('Coroutine finished')\n",
    "\n",
    "async def main():\n",
    "    await my_coroutine()\n",
    "\n",
    "# 在 Jupyter Notebook 中，直接使用 await 運行 main()\n",
    "await main()"
   ]
